{
  "expo": {
    "name": "baton",
    "description": "潜在意識を呼び起こすサポートアプリ",
    "privacy": "public",
    "sdkVersion": "27.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.3",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "orientation": "portrait",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mankutsu.baton"
    },
    "android": {
      "package": "com.mankutsu.baton",
      "versionCode": 2
    }
  }
}
